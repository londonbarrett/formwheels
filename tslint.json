{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}